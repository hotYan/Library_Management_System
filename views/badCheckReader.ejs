<%- include badHeader %>
  <link rel="stylesheet" type="text/css" href="../stylesheets/searchTable.css">
  <script type="text/javascript">
    $(document).ready(function () {
      $(".left ul li:eq(3)").css("color", "#FFF");
      $(".left ul li:eq(3)").css("background-color", "#B78DE7");
      $(".left ul").find('li:eq(3)').children(".trig").css('display', 'block');
      $(".table tbody tr:odd").css("backgroundColor", "#E3F2F5");
    });
  </script>
  <%- include badLeftBar %>
    <div class="ad_data">
      <form class="search" action="/badCheckReader" method="POST">
        <input type="text" placeholder="输入读者借阅号" name="UserId" />
        <button class="btn" type="submit">搜索</button>
      </form>
      <div class="table_div">
        <table class="table">
          <thead>
            <tr>
              <th class="tdOne">借阅号</th>
              <th class="tdOne">用户名</th>
              <th>学院</th>
              <th>专业</th>
              <th>电话</th>
              <th class="tdOne">邮箱</th>
              <th>借阅上限</th>
              <th>借阅期限</th>
              <th>在借数量</th>
            </tr>
          </thead>
          <tbody>
            <% for(var i=0;i< datas.length;i++){ %>
              <tr>
                <td class="tdOne">
                  <%= datas[i].UserId %>
                </td>
                <td class="tdOne">
                  <%= datas[i].UserName %>
                </td>
                <td>
                  <%= datas[i].Departments %>
                </td>
                <td>
                  <%= datas[i].Major %>
                </td>
                <td>
                  <%= datas[i].Phone %>
                </td>
                <td class="tdOne">
                  <%= datas[i].Email %>
                </td>
                <td>
                  <%= datas[i].Max %>
                </td>
                <td>
                  <%= datas[i].Time %>
                </td>
                <td>
                  <%= datas[i].LendedNum %>
                </td>
              </tr>
              <% }%>

          </tbody>
        </table>
      </div>
    </div>
    </div>
    
    </div>
    </div>
    </body>

    </html>